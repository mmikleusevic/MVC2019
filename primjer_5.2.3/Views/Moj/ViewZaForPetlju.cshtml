@model string[]
@{
    ViewBag.Title = "ViewZaForPetlju";
}

<h2>ViewZaForPetlju</h2>

<ul>
    @foreach (var item in Model)
    {
        <li>@item</li>
    }
</ul>
<ul>
    @for (int i = 0; i < Model.Length; i++)
    {
        <li>@Model[i]</li>
    }
</ul>

@{
    int b = Model.Length - 1;
}
<ul>
    @while (b >= 0)
    {
        {
            <li>@Model[b]</li>
            b--;
        }
    }
</ul>

@{
    int k = 0;
}
<ul>
    @while (k < Model.Length)
    {
        {
            <li>@Model[k]</li>
            k++;
        }
    }
</ul>
