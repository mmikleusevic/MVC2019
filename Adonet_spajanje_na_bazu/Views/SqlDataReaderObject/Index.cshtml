@model List<Adonet_spajanje_na_bazu.Models.Tecaj>
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <table>
        <thead>
            <tr>
                <th>Naziv</th>
                <th>Opis</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.Naziv</td>
                    <td>@item.Opis</td>
                </tr>
            }
        </tbody>
    </table>
</body>
</html>
